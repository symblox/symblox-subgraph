{
  "name": "symblox-subgraph",
  "license": "MIT",
  "version": "1.0.0",
  "author": "Symblox",
  "main": "index.js",
  "browser": "index.min.js",
  "scripts": {
    "lint": "eslint .",
    "yaml:rates": "node ./create-yaml-file create-yaml --subgraph rates --env prod --universal-test-block null | mustache - mustache/yaml.mustache > subgraphs/symblox-rates.yaml",
    "yaml:exchanger": "node ./create-yaml-file create-yaml --subgraph exchanger --env prod --universal-test-block null | mustache - mustache/yaml.mustache > subgraphs/symblox-exchanger.yaml",
    "codegen": "graph codegen",
    "codegen:rates": "yarn yaml:rates && yarn codegen -- subgraphs/symblox-rates.yaml",
    "codegen:exchanges": "yarn codegen -- subgraphs/symblox-exchanges.yaml",
    "codegen:exchanger": "yarn yaml:exchanger && yarn codegen -- subgraphs/symblox-exchanger.yaml",
    "build": "graph build",
    "build:rates": "graph build subgraphs/symblox-rates.yaml",
    "build:exchanges": "graph build subgraphs/symblox-exchanges.yaml",
    "build:orders": "graph build subgraphs/synthetix-limit-orders.yaml",
    "build:exchanger": "graph build subgraphs/symblox-exchanger.yaml",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ --access-token $THEGRAPH_SNX_ACCESS_TOKEN",
    "deploy:rates": "yarn deploy -- symblox/symblox-rates subgraphs/symblox-rates.yaml",
    "deploy:exchanges": "yarn deploy -- symblox/symblox-exchanges subgraphs/symblox-exchanges.yaml",
    "deploy:orders": "yarn deploy -- symblox/synthetix-limit-orders subgraphs/synthetix-limit-orders.yaml",
    "deploy:exchanger": "yarn deploy -- symblox/symblox-exchanger subgraphs/symblox-exchanger.yaml",
    "create-local": "graph create --node http://localhost:8020/ symblox/bpool",
    "remove-local": "graph remove --node http://localhost:8020/ symblox/bpool",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 symblox/bpool"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.19.0",
    "@graphprotocol/graph-ts": "0.19.0",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "mustache": "4.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "minify": "4.1.3",
    "prettier": "1.17.1",
    "typescript": "3.8.3"
  }
}
